{
  "connection": "default",
  "collectionName": "properties",
  "info": {
    "name": "Property"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Type": {
      "type": "string"
    },
    "Furnished": {
      "type": "boolean"
    },
    "Size": {
      "type": "integer"
    },
    "FrontPicture": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "PropertyPictures": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "Rented": {
      "type": "enumeration",
      "enum": [
        "yes",
        "no",
        "onhold"
      ]
    },
    "RoomTitle": {
      "type": "string"
    },
    "RoomText": {
      "type": "string"
    },
    "Rent": {
      "type": "string"
    },
    "Description": {
      "type": "richtext"
    },
    "Period": {
      "type": "string"
    }
  }
}
